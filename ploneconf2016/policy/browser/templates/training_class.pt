<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      metal:use-macro="context/main_template/macros/master">

    <metal:block fill-slot="content-title" />
    <metal:block fill-slot="content-description" />

    <metal:block fill-slot="content-core">
        <h1>${context/Title}</h1>
        <h3>${context/summary}<h3>
        <h4>presented by: ${context/instructor}</h4>
        <h4>${context/body}</h4>
        <h4><b><span>Duration of Training: </span></b><span>${context/duration}</span></h4>
        <h4><b><span>Target Level: </span></b><span>${context/level}</span></h4>
        <h4><b><span>Target Audience: </span></b><span>${context/audience}</span></h4>
    </metal:block>

</html>
